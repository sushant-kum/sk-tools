{
  "version": 1,
  "projects": {
    "frontend-auth": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/frontend/auth",
      "sourceRoot": "apps/frontend/auth/src",
      "prefix": "auth",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/frontend/auth",
            "index": "apps/frontend/auth/src/index.html",
            "main": "apps/frontend/auth/src/main.ts",
            "polyfills": "apps/frontend/auth/src/polyfills.ts",
            "tsConfig": "apps/frontend/auth/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/frontend/auth/src/assets"],
            "styles": ["apps/frontend/auth/src/styles/index.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/frontend/auth/src/environments/environment.ts",
                  "with": "apps/frontend/auth/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "frontend-auth:build",
            "proxyConfig": "apps/frontend/auth/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "frontend-auth:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "frontend-auth:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/frontend/auth/tsconfig.app.json", "apps/frontend/auth/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/frontend/auth/**/*"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/frontend/auth/src/test.ts",
            "tsConfig": "apps/frontend/auth/tsconfig.spec.json",
            "karmaConfig": "apps/frontend/auth/karma.conf.js",
            "polyfills": "apps/frontend/auth/src/polyfills.ts",
            "styles": [],
            "scripts": [],
            "assets": []
          }
        }
      }
    },
    "frontend-auth-e2e": {
      "root": "apps/frontend/auth-e2e",
      "sourceRoot": "apps/frontend/auth-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/frontend/auth-e2e/cypress.json",
            "tsConfig": "apps/frontend/auth-e2e/tsconfig.e2e.json",
            "devServerTarget": "frontend-auth:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "frontend-auth:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/frontend/auth-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/frontend/auth-e2e/**/*"]
          }
        }
      }
    },
    "api-auth": {
      "root": "apps/api/auth",
      "sourceRoot": "apps/api/auth/src",
      "projectType": "application",
      "prefix": "api-auth",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api/auth",
            "main": "apps/api/auth/src/main.ts",
            "tsConfig": "apps/api/auth/tsconfig.app.json",
            "assets": ["apps/api/auth/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/auth/src/environments/environment.ts",
                  "with": "apps/api/auth/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api-auth:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/api/auth/tsconfig.app.json", "apps/api/auth/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/api/auth/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/auth/jest.config.js",
            "tsConfig": "apps/api/auth/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "frontend-applications": {
      "projectType": "library",
      "root": "libs/frontend/applications",
      "sourceRoot": "libs/frontend/applications/src",
      "prefix": "applicationsapplications",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/frontend/applications/tsconfig.lib.json",
              "libs/frontend/applications/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/frontend/applications/**/*"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/frontend/applications/src/test.ts",
            "tsConfig": "libs/frontend/applications/tsconfig.spec.json",
            "karmaConfig": "libs/frontend/applications/karma.conf.js"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  }
}
